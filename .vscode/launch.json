{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run train.py, RWA",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run_train.py",
            "args": [
                "-S2",
                "-NIR",
                "-S1",
                "-treg", "rwa",
                "-tregtrain", "rwa",
                "--seed", "1600",
                "-occmodel",
                "-wd", "0.00001",
                "-senbuilds",
                "-pret",
                "--biasinit", "0.9407", 
            ],
            "console": "integratedTerminal"
        },{
            "name": "Run train.py, PRI",
            "type": "python",
            "request": "launch",
